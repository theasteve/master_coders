<%= form_for [@proposal, @experiment] do |f| %>
<p>
  <%= f.label :name %>
  <%= f.text_field :name %>
</p>

<p>
  <%= f.label :description %>
  <%= f.text_area :description %>
</p>

<p>
  <%= f.label :results %>
  <%= f.text_area :results %>
</p>

<p>
  <%= f.label :conclusion %>
  <%= f.text_area :conclusion %>
</p>

<p>
  <%= f.submit %>
</p>

<% end %>
